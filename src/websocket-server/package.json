{
  "name": "service-translate-websocket-server",
  "version": "1.0.0",
  "description": "Local WebSocket server for Service Translate client communication",
  "main": "dist/websocket-server/src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/websocket-server/src/server.js",
    "dev": "tsc && node dist/websocket-server/src/server.js",
    "watch": "tsc -w",
    "test": "node test-client.js",
    "test:cognito-integration": "tsc && node dist/websocket-server/src/test-session-cognito-integration.js"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.901.0",
    "@aws-sdk/client-polly": "^3.901.0",
    "@aws-sdk/credential-providers": "^3.901.0",
    "@types/ws": "^8.18.1",
    "amazon-cognito-identity-js": "^6.3.15",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "proper-lockfile": "^4.1.2",
    "socket.io": "^4.7.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/uuid": "^11.0.0",
    "socket.io-client": "^4.7.5",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "websocket",
